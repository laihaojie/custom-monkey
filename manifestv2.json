{
    "name": "custom-monkey",
    "description": "Hello world Extension",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "mao.png"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "permissions": [
        "webNavigation",
        "tabs",
        "scripting"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}
